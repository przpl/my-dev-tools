{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES2020",
        "outDir": "../../out/test",
        "lib": ["ES2020"],
        "sourceMap": true,
        "rootDir": "..",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules", ".vscode-test"]
}
